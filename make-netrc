#!/bin/bash
GITUSER=$(aws ssm get-parameter --name /excellaco/user --query 'Parameter.Value' --out text)
GITPASS=$(aws ssm get-parameter --name /excellaco/tkn --query 'Parameter.Value' --with-decryption --out text)
cat << EOF > .netrc
# Auto-generated by make-netrc $(date)
machine github.com
  login $GITUSER
  password $GITPASS
EOF
